<Page x:Class="auction_central.NPHome"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:auction_central"
      mc:Ignorable="d" 
      d:DesignHeight="900" d:DesignWidth="1300" Background="White"
      Title="NonPHome">

    <Grid>
        <Frame x:Name="npHome"/>
        <Button x:Name="eventDetails_button" Content="My Auctions" HorizontalAlignment="Left" Margin="40,193,0,0" VerticalAlignment="Top" Width="280" Height="51" FontSize="26.667" Click="eventDetails_button_Click"/>
        <Button x:Name="button1" Content="Make Event Request" HorizontalAlignment="Left" Margin="40,272,0,0" VerticalAlignment="Top" Width="280" Height="51" FontSize="26.667" Click="button1_Click"/>
        <TextBlock x:Name="np_header" HorizontalAlignment="Left" Height="88" Margin="340,66,0,0" TextAlignment="Center" TextWrapping="Wrap" Text="Welcome, Non-Profit." VerticalAlignment="Top" Width="620" FontSize="64" FontFamily="Segoe UI Light"/>

    </Grid>
</Page>
