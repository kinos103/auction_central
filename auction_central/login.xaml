<Page x:Class="auction_central.login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:auction_central"
      mc:Ignorable="d" 
      d:DesignHeight="900" d:DesignWidth="1300" Background="White"
      Title="login">

    <Grid>
        <Frame x:Name="Login"></Frame>
        <Grid Margin="478,242,613,382">
            <TextBox x:Name="login_email" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="email" VerticalAlignment="Top" Width="209"/>
            <TextBox x:Name="login_password" HorizontalAlignment="Left" Height="26" Margin="0,48,0,0" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="209"/>
            <Button x:Name="login_loginButton" Content="LOGIN" HorizontalAlignment="Left" Margin="70,122,0,0" VerticalAlignment="Top" Width="75"/>
            <TextBlock x:Name="login_haveAccountText" HorizontalAlignment="Left" Margin="40,206,0,0" TextWrapping="Wrap" Text="Dont have an account?" VerticalAlignment="Top"/>
            <Button x:Name="login_createAccountButton" Content="Create Account" HorizontalAlignment="Left" Height="32" Margin="16,244,0,0" VerticalAlignment="Top" Width="146"/>

        </Grid>

    </Grid>
</Page>
